    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <ImageView fitHeight="768.0" fitWidth="1024.0">
               <image>
                  <Image url="@thumb-1920-63312.jpg" />
               </image>
            </ImageView>
